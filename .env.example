# Solana RPC
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WS_URL=wss://api.mainnet-beta.solana.com

# Wallet (base58 encoded private key or path to keypair file)
WALLET_PRIVATE_KEY=
WALLET_KEYPAIR_PATH=

# Database
DATABASE_URL=postgresql://clawops:clawops@localhost:5432/clawops

# Redis
REDIS_URL=redis://localhost:6379

# API
API_HOST=0.0.0.0
API_PORT=3100

# Risk defaults
MAX_POSITION_SIZE_SOL=1.0
MAX_SLIPPAGE_BPS=300
MAX_PRIORITY_FEE_LAMPORTS=100000
EXECUTION_COOLDOWN_MS=5000

# Agent Swarm (set SWARM_ENABLED=true to activate)
SWARM_ENABLED=false
LLM_PROVIDER=anthropic
LLM_API_KEY=
LLM_MODEL=claude-sonnet-4-5-20250929
LLM_MAX_TOKENS=1024
NODE_ID=node-1
INTEL_CHANNEL_PREFIX=cyclawps

# Logging
LOG_LEVEL=info
NODE_ENV=development
